[application]

config/name="SimpleTiles"
config/description="a simple top-down 2D game where you walk around and do things"
run/main_scene="res://MenuRoot.tscn"
config/icon="res://icon.png"
config/version="0.1.2"

[audio]

master_volume=50
music_volume=100
footstep_volume=100

[autoload]

Globals="*res://Scripts/autoload/Globals.gd"
